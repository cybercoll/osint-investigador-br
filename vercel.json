{
  "version": 2,
  "builds": [
    {
      "src": "web_app_production.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_app_production.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_ENV": "production"
  },
  "functions": {
    "web_app_production.py": {
      "maxDuration": 30
    }
  }
}